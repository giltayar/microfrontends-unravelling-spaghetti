{
  "name": "@roundforest/todo-list-page-server",
  "version": "1.0.0",
  "description": "",
  "type": "module",
  "exports": "./src/todo-list-page-server-server.js",
  "main": "./src/todo-list-page-server-server.js",
  "bin": {
    "todo-list-page-server": "src/run-todo-list-page-server-server.js"
  },
  "types": "./types/src/todo-list-page-server-server.d.ts",
  "engines": {
    "node": ">=15.0.0"
  },
  "files": ["src", "types", "dist"],
  "publishConfig": {
    "access": "restricted",
    "registry": "https://npm.pkg.github.com"
  },
  "config": {
    "docker_node_version": "15.5.1"
  },
  "scripts": {
    "start": "node src/run-todo-list-page-server-server.js",
    "prebuild": "rollup -c",
    "build": "concurrently -r 'npm:build:*'",
    "build:typescript": "rm -rf types && tsc && cp src/*.d.ts types/src",
    "build:docker": "DOCKER_BUILDKIT=1 docker build -t roundforestltd/todo-list-page-server-server:${npm_package_version} --secret id=npmrc,src=$(npm get userconfig) --build-arg NODE_VERSION=${npm_package_config_docker_node_version} .",
    "test": "FULL_TEST=1 concurrently -r 'npm:test:*'",
    "test:mocha-parallel": "npm run mocha -- --parallel",
    "test:eslint": "eslint --max-warnings=0 --color '**/*.js'",
    "test:typescript": "tsc --noEmit --emitDeclarationOnly false --pretty",
    "postpublish": "concurrently -r 'npm:publish:*'",
    "publish:docker": "docker push roundforestltd/todo-list-page-server-server:${npm_package_version}",
    "mocha": "mocha --no-timeouts --exit --color 'test/unit/*.test.js' 'test/integ/*.test.js' 'test/e2e/*.test.js'"
  },
  "keywords": ["javascript", "nodejs"],
  "author": "Gil Tayar <giltayar@roundforest.com>",
  "license": "MIT",
  "repository": {
    "url": "https://github.com/roundforest/mono",
    "directory": "packages/todo-list-page-server-server",
    "type": "git"
  },
  "devDependencies": {
    "@babel/core": "^7.13.10",
    "@babel/eslint-parser": "^7.13.10",
    "@babel/plugin-syntax-top-level-await": "^7.12.13",
    "@emotion/babel-plugin": "^11.2.0",
    "@rollup/plugin-babel": "^5.3.0",
    "@rollup/plugin-commonjs": "^17.1.0",
    "@rollup/plugin-node-resolve": "^11.2.0",
    "@rollup/plugin-replace": "^2.4.1",
    "@roundforest/docker-compose-testkit": "^1.5.23",
    "@roundforest/http-commons": "^3.0.7",
    "@roundforest/mocha-commons": "^1.0.11",
    "@roundforest/pino-testkit": "^2.0.6",
    "@roundforest/promise-commons": "^1.1.7",
    "@roundforest/scripting-commons": "^1.8.4",
    "@roundforest/webdriverio-testkit": "^1.1.33",
    "@types/chai": "^4.2.15",
    "@types/chai-subset": "^1.3.3",
    "@types/mocha": "^8.2.2",
    "@types/node": "^14.14.35",
    "@types/react-dom": "^17.0.2",
    "@types/styled-components": "^5.1.9",
    "@types/uuid": "^8.3.0",
    "babel-plugin-htm": "^3.0.0",
    "babel-plugin-styled-components": "^1.12.0",
    "chai": "^4.3.4",
    "chai-subset": "^1.6.0",
    "concurrently": "^6.0.0",
    "eslint": "^7.22.0",
    "eslint-plugin-mocha-no-only": "^1.1.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^3.3.1",
    "mocha": "^9.1.3",
    "p-retry": "^4.4.0",
    "prettier": "^2.2.1",
    "rollup-plugin-terser": "^7.0.2",
    "typescript": "^4.2.3",
    "uuid": "^8.3.2"
  },
  "dependencies": {
    "@emotion/cache": "^11.1.3",
    "@emotion/css": "^11.1.3",
    "@emotion/react": "^11.1.5",
    "@emotion/server": "^11.0.0",
    "@emotion/styled": "^11.1.5",
    "@roundforest/emotion-styled": "^1.0.4",
    "@roundforest/frontend-commons": "^1.0.1",
    "@roundforest/functional-commons": "^1.4.2",
    "@roundforest/log-request-fastify-plugin": "^1.1.28",
    "@roundforest/pino-global": "^2.2.31",
    "@roundforest/request-id-fastify-plugin": "^1.0.14",
    "@roundforest/request-id-logger-fastify-plugin": "^1.0.29",
    "close-with-grace": "^1.1.0",
    "fastify": "^3.14.0",
    "fastify-helmet": "^5.3.1",
    "fastify-static": "^4.0.1",
    "htm": "^3.0.4",
    "pino": "^6.11.2",
    "react": "^17.0.1",
    "react-dom": "^17.0.1"
  }
}
