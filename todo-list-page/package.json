  "exports": "./lib/src/todo-list-page.js",
  "main": "./lib/src/todo-list-page.js",
  "types": "./types/src/todo-list-page.d.ts",
  "engines": {
    "node": ">=15.0.0"
  },
  "files": ["src", "lib/src", "types/src"],
  "publishConfig": {
    "access": "restricted",
    "registry": "https://npm.pkg.github.com"
  },
  "watch": {
    "mocha:unit": {
      "patterns": ["test/unit"],
      "extensions": "tsx"
    }
  },
  "scripts": {
    "build": "concurrently -r 'npm:build:*'",
    "build:typescript": "rm -rf types && tsc",
    "test": "FULL_TEST=1 concurrently -r 'npm:test:*'",
    "test:mocha-parallel": "npm run mocha -- --parallel",
    "test:eslint": "eslint --color --max-warnings=0 '**/*.[t|j]s?(x)'",
    "test:typescript": "tsc --noEmit --pretty",
    "mocha": "mocha --loader=./.loaders.mjs --no-timeouts --exit --color 'test/unit/**/*.test.[t|j]s?(x)' 'test/integ/**/*.test.[t|j]s?(x)' 'test/e2e/**/*.test.[t|j]s?(x)'",
    "mocha:e2e": "mocha --loader=./.loaders.mjs --no-timeouts --exit --color 'test/e2e/**/*.test.[t|j]s?(x)'",
    "mocha:unit": "mocha --loader=./.loaders.mjs --no-timeouts --exit --color 'test/unit/**/*.test.[t|j]s?(x)'",
    "watch:mocha:unit": "npm-watch mocha:unit",
    "play": "rollup --watch --config play/rollup.config.mjs"
  },
  "keywords": ["javascript", "nodejs"],
  "author": "Gil Tayar <giltayar@roundforest.com>",
  "license": "MIT",
  "repository": {
    "url": "https://github.com/roundforest/mono",
    "directory": "packages/todo-list-page-component",
    "type": "git"
  },
